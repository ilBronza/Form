{
    "name": "ilbronza/form",
    "description": ":form creator",
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Bronzini",
            "email": "davide_bronzini@hotmail.com",
            "homepage": "https://sistema.srl"
        }
    ],
    "homepage": "https://github.com/ilbronza/form",
    "keywords": ["Laravel", "Form"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ilBronza\\Form\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ilBronza\\Form\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ilBronza\\Form\\FormServiceProvider"
            ],
            "aliases": {
                "Form": "ilBronza\\Form\\Facades\\Form"
            }
        }
    }
}
